{
  "name": "redis-fans",
  "description": "A Redis GUI client, build on Electron & React & Material UI.",
  "version": "0.1.2-preview",
  "main": "main.bundle.js",
  "repository": "git@gitee.com:jiyuan0125/redis-fans.git",
  "author": "Su Jiyuan <jiyuan0125@126.com>",
  "license": "MIT",
  "scripts": {
    "dev-main": "cross-env NODE_ENV=development ELECTORN_RENDERER_PORT=28118 electron -r ts-node/register src/main/main.ts --remote-debugging-port=9222",
    "dev-renderer": "cross-env NODE_ENV=development ELECTORN_RENDERER_PORT=28118 webpack-dev-server --config config/webpack.renderer.config.js",
    "build-rediscli": "cross-env NODE_ENV=production webpack --config config/webpack.rediscli.config.js",
    "build-main": "cross-env NODE_ENV=production webpack --config config/webpack.main.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --env.prod --config config/webpack.renderer.config.js",
    "build-all": "yarn build-rediscli && yarn build-main && yarn build-renderer",
    "postinstall": "electron-builder install-app-deps",
    "dist": "electron-builder -m"
  },
  "dependencies": {
